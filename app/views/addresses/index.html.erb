<h1>Listing Addresses</h1>
 
<table>
  <tr>
    <th>Name</th>
    <th>Phone Number</th>
    <th>Email</th>
    <th>Name</th>
    <th>Address</th>
  </tr>
 
  <% @address.each do |address| %>
    <tr>
      <td><%= address.name %></td>
      <td><%= address.Phone_Number %></td>
      <td><%= address.email %></td>
      <td><%= address.address %></td>
      <td><%= link_to 'Show', address_path(address) %></td>
      <td><%= link_to 'Create', address_path(address) %></td>
    </tr>
  <% end %>
</table>